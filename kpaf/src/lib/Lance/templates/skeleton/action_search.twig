{% for filter, filterSource in blockInfo.condition %}
{% if not filterSource is array %}
{% set field = schemaSet.getField(filterSource) %}
{% if field.isReferenceField %}
{% set entityOrigName = field.getEntity.getReferencedEntityName(field.getConfiguredName) %}
{% set entityName = schemaSet.getEntityExportName(entityOrigName) %}
{% set entity = schemaSet.getUsedEntity(entityName) %}
{% if entity.isHardCoded %}
        ${{ field.getConfiguredName }} = \{{ schemaSet.getNamespace }}\Model\{{ schemaSet.getSchemaSetName|pascal }}\{{ entityName|pascal }}::getDictionary();
{% endif %}
{% endif %}
{% endif %}
{% endfor %}